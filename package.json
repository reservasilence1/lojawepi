{
  "name": "wepink-clone",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && npm run copy-files",
    "build:basepath": "set USE_BASEPATH=true&& next build && npm run copy-files",
    "build:local": "next build && npm run copy-files",
